
include "%L"

### also check /usr/share/X11/locale/en_US.UTF-8/Compose

## Cheatsheet
 #    • period, equal
 #    ¿ question, question
 #    ¡ bang, bang
 #    Ø O slash
 #    … period, period
 #    « less, less
 #    » more, more
 #    — minus, minus, minus
 #    ‽ bang, question
 #    → dash, more
 #    ← less, dash
 #    ° o, o
 #    ♥ less, 3


<Multi_key> <n> <n> : "ñ"
<Multi_key> <l> <l> : "λ"
<Multi_key> <N> <N> : "Ñ"

# emojis
<Multi_key> <e> <r> : "🐇"  # rabbit
<Multi_key> <e> <h> : "💕"  # hearts (two)
<Multi_key> <e> <b> : "🍌"  # banana
<Multi_key> <e> <o> : "💯"  # 100
<Multi_key> <e> <m> : "🐁"  # mouse
<Multi_key> <e> <c> : "😭"  # crying
<Multi_key> <e> <l> : "🦞"  # lobster
<Multi_key> <e> <k> : "🥬"  # kale
<Multi_key> <e> <f> : "🔥"  # fire
<Multi_key> <e> <a> : "🐜"  # ant
<Multi_key> <e> <s> : "⚽"  # soccer ball

